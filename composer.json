{
    "name": "php-solution/doctrine-orm-extra-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provides an additional functionality for work with doctrine2 ",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikita Kalinin",
            "email": "kalinick@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "doctrine/orm": "^2.5",
        "php-solution/doctrine-orm-extra-lib": "^0.3",
        "symfony/config": "^3.3",
        "symfony/dependency-injection": "^3.3",
        "symfony/http-kernel": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0||~6.0",
        "symfony/doctrine-bridge": "^3.3",
        "symfony/form": "^3.3",
        "symfony/http-foundation": "^3.3"
    },
    "suggest": {
    },
    "autoload":     {
        "psr-4": { "PhpSolution\\Doctrine\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "phpunit"
    }
}